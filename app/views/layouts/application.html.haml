!!!
%html{:lang=>'en'}
  = render 'layouts/head'
  %body
    .container
      .navbar.navbar-fixed-top
        .navbar-inner
          %a.brand{:href=>'/'} Home
          %ul.nav
            - if user_signed_in?
              %li= link_to 'Sign Out', logout_path
            - else
              %li= link_to 'Sign In', login_path
      = yield
